{
  "name": "h5p-bildetema",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "vite build --mode=development --watch --minify=false",
    "start:dev": "vite",
    "build:dev": "vite build --mode=development && npm run generate-semantics && npm run generate-library",
    "build": "vite build && npm run generate-semantics && npm run generate-library",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint . --fix",
    "lint": "npm run format:check && npm run eslint:check",
    "lint:fix": "npm run format:fix && npm run eslint:fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "storybook": "start-storybook -p 6006 -h localhost",
    "build-storybook": "build-storybook",
    "generate-semantics": "h5p-generate-configs s --type-definition ./src/semantics.ts --out ./semantics.json --translations ./src/types/TranslationKey.ts",
    "//watch-semantics": "nodemon ./src/**/* --exec 'npm run generate-semantics'",
    "generate-library": "h5p-generate-configs l --type-definition ./src/library.ts --out ./library.json",
    "//watch-library": "nodemon ./src/**/* --exec 'npm run generate-library'"
  },
  "dependencies": {
    "@mdx-js/react": "^2.1.1",
    "h5p-utils": "^2.0.6",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/builder-vite": "^0.1.35",
    "@storybook/react": "^6.5.5",
    "@types/jquery": "^3.5.14",
    "@types/node": "^17.0.36",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-react": "^1.3.0",
    "h5p-generate-configs": "^0.2.0",
    "h5p-types": "^0.0.23",
    "nodemon": "^2.0.15",
    "sass": "^1.50.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.7.2",
    "vite": "^2.9.1",
    "vitest": "^0.12.9"
  }
}
