{
  "name": "h5p-bildetema",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "vite build --mode=development --watch --minify=false",
    "start:dev": "vite",
    "build:dev": "vite build --mode=development && npm run generate-semantics && npm run generate-library",
    "build": "tsc && vite build && npm run generate-semantics && npm run generate-library",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint . --fix",
    "lint": "npm run format:check && npm run eslint:check",
    "lint:fix": "npm run format:fix && npm run eslint:fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "storybook": "start-storybook -p 6006 -h localhost",
    "build-storybook": "build-storybook",
    "generate-semantics": "h5p-generate-configs s --type-definition ./src/semantics.ts --out ./semantics.json --translations ./src/types/TranslationKey.ts",
    "//watch-semantics": "nodemon ./src/**/* --exec 'npm run generate-semantics'",
    "generate-library": "h5p-generate-configs l --type-definition ./src/library.ts --out ./library.json",
    "//watch-library": "nodemon ./src/**/* --exec 'npm run generate-library'",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@mdx-js/react": "^2.3.0",
    "@tanstack/react-query": "^4.29.19",
    "h5p-types": "^2.0.1",
    "h5p-utils": "^3.2.2",
    "react": "^18.2.0",
    "react-detect-click-outside": "^1.1.7",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1",
    "react-to-print": "^2.14.13",
    "react-toggle": "^4.1.3",
    "use-h5p": "^1.3.1",
    "use-react-router-breadcrumbs": "^4.0.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.24",
    "@storybook/addon-links": "^7.0.24",
    "@storybook/builder-vite": "^7.0.24",
    "@storybook/react": "^7.0.24",
    "@testing-library/react": "^14.0.0",
    "@types/jquery": "^3.5.16",
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "^4.0.1",
    "esbuild": "^0.18.11",
    "h5p-generate-configs": "^0.4.2",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vitest": "^0.32.4"
  }
}
